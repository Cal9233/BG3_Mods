    entity = entity or context.Target
    result = Dead(entity) & ~Party() & ~Tagged('PLAYABLE') & ~Tagged('AVATAR') & ~Tagged('CONSTRUCT')  & ~Tagged('ELEMENTAL') & ~Tagged('PLANT') & ~Tagged('OOZE') & ~HasStatus('CRAWLING_GNAW_TECHNICAL')
    return ConditionResult(result.Result, {ConditionError("IsNotTargetableCorpse")})
end

